{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "files": ["**/*"],
  "ignorePaths": [
    "**/*.svg",
    "**/__snapshots__/**",
    ".*ignore",
    "/*.code-workspace",
    "/.git",
    "/packages/app/.env.*.local",
    "/packages/app/.env.local",
    "/packages/app/.env.site.*",
    "/pnpm-lock.yaml"
  ],
  "includeRegExpList": ["[ -~]"],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "antfu",
    "commitizen",
    "jiti",
    "jsdoc",
    "nums",
    "nvmrc",
    "pathe",
    "tsdoc",
    "vite",
    "zustand"
  ]
}
